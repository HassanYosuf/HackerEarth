import java.util.*;
class TestClass {
    public static void main(String args[] ) throws Exception {
        
        // Write your code here
    Scanner s = new Scanner(System.in);
    try{
        int t = s.nextInt();
        
        while(t-->0)
        {
            int SH,SM,EH,EM;
            SH=s.nextInt();
            SM=s.nextInt();
            EH=s.nextInt();
            EM=s.nextInt();
            
            if(EM<SM)
                System.out.println((EH-SH-1)+" "+(EM+60-SM));
            else 
                System.out.println((EH-SH)+" "+(EM-SM));
        }

    }catch(Exception e){}


    }
}
