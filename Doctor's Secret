import java.util.*;
class TestClass {
    public static void main(String args[] ) throws Exception {
    

        // Write your code here
        Scanner s = new Scanner(System.in);

        int l = s.nextInt();
        int n = s.nextInt();

        if(l<=23&&(n>=500&&n<=1000))
        System.out.print("Take Medicine");
        else
        System.out.print("Don't take Medicine");

    }
}
