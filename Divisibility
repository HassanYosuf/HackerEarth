import java.util.*;
class TestClass {
    public static void main(String args[] ) throws Exception {
       

        // Write your code here
      try
      {
         Scanner s = new Scanner(System.in);
         int n = s.nextInt();
         long[] a = new long[n];

         for(int i = 0;i<n;i++)
             a[i]=s.nextInt();
              
         if(a[a.length-1]%10==0)
         System.out.println("Yes");
         else
         System.out.println("No");

      }catch(Exception e){}
    }
}
